{
    "version": 2,
    "builds": [
        { "src": "dist/index.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "routes": [
                { "src": "/api/(.*)", "dest": "dist/api.js" },
                { "src": "/(.*)", "dest": "dist/index.js" }
            ]
        }
    ]
}
